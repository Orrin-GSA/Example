{
  "name": "dashboard-template",
  "version": "0.1.0",
  "private": true,
  "source": "src_react/index.html",
  "alias": {
    "xlsx": {
      "global": "XLSX"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.2.5",
    "@tanstack/match-sorter-utils": "^8.15.1",
    "@tanstack/react-table": "^8.19.2",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.8",
    "chartjs-plugin-datalabels": "^2.2.0",
    "file-saver": "^2.0.5",
    "formik": "^2.4.6",
    "parchment": "^3.0.0",
    "query-string": "^9.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-form-stepper": "^2.0.3",
    "react-multi-select-component": "^4.3.4",
    "react-quilljs": "^2.0.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.30.0",
    "react-select": "^5.10.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^11.0.2",
    "sheetjs-style": "^0.15.8",
    "web-vitals": "^2.1.4",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yup": "^1.4.0"
  },
  "scripts": {
    "pullconfig": "node downloadDbConfig.js",
    "pushconfig": "node uploadDbConfig.js",
    "devlocal": "npm run clean && concurrently \"npm run start_local\" \"npm run mock_api\"",
    "build": "npm run clean && cd src_scripts && npm run build && cd ../ && npm run build_parcel_opt && node src_dev/fixBuild.js",
    "build_dev": "npm run clean && cd src_scripts && npm run build && cd ../ && npm run build_parcel && node src_dev/fixBuild.js",
    "build_parcel": "cross-env NODE_ENV=development parcel build --no-optimize --no-source-maps --no-content-hash src_react/index.html --dist-dir ./dist",
    "build_parcel_opt": "cross-env NODE_ENV=production parcel build --no-source-maps --no-content-hash src_react/index.html --dist-dir ./dist",
    "gpush": "clasp push --force",
    "publish_prod": "node src_dev/publishProd.js",
    "start_local": "parcel ./src_react/index_dev.html --dist-dir ./dist",
    "mock_api": "cd src_dev && npm start",
    "clean": "del-cli ./dist ./.parcel-cache",
    "postinstall": "cd src_dev && npm install && cd ../src_scripts && npm install"
  },
  "parcel-namer-rewrite": {
    "hashing": "never"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.13.3",
    "@parcel/transformer-sass": "^2.13.2",
    "@types/google-apps-script": "^1.0.83",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "express": "^4.19.2",
    "nodemon": "^3.0.2",
    "parcel": "^2.13.2",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "process": "^0.11.10",
    "typescript": "^5.5.4"
  }
}
