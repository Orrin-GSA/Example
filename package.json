{
  "name": "react_clasp",
  "version": "1.0.1",
  "description": "Project Monitor",
  "scripts": {
    "gin": "clasp login",
    "gout": "clasp logout",
    "gstart": "clasp push --force --watch",
    "gpush": "clasp push --force",
    "clean": "del-cli ./dist ./.parcel-cache",
    "build_parcel": "SET NODE_ENV=development&& parcel build --no-optimize --no-source-maps --no-content-hash src_react/index.html --dist-dir ./dist",
    "build_parcel_opt": "SET NODE_ENV=production&& parcel build --no-source-maps --no-content-hash src_react/index.html --dist-dir ./dist",
    "build": "npm run clean && cd src && npm run build && cd ../ && npm run build_parcel_opt && node src_dev/fixBuild.js",
    "build_dev": "npm run clean && cd src && npm run build && cd ../ && npm run build_parcel && node src_dev/fixBuild.js",
    "build_app_test": "SET NODE_ENV=production&& npm run build",
    "start": "parcel src_react/index.html --dist-dir ./dist",
    "start_local": "parcel src_react/index_dev.html --dist-dir ./dist",
    "rpa_mock": "cd src_dev && npm start",
    "dev": "npm run clean && concurrently \"npm run gstart\" \"npm run start\"",
    "devlocal": "npm run clean && concurrently \"npm run start_local\" \"npm run rpa_mock\"",
    "lint": "eslint --quiet",
    "lint-verbose": "eslint",
    "publish_test": "node src_dev/publishTest.js",
    "publish_dev": "node src_dev/publishDev.js",
    "publish_prod": "node src_dev/publishProd.js",
    "push": "node src_dev/pushChanges.js",
    "increment_build": "node src_dev/incrementBuildNo.js",
    "postinstall": "cd src_dev && npm install && cd ../src && npm install"
  },
  "author": "MSI",
  "license": "ISC",
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "alias": {
    "xlsx": {
      "global": "XLSX"
    }
  },
  "parcel-namer-rewrite": {
    "hashing": "never"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@parcel/resolver-glob": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@types/jquery": "^3.5.29",
    "@types/luxon": "^3.4.2",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "del-cli": "^6.0.0",
    "eslint": "^9.12.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "parcel": "^2.12.0",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "process": "^0.11.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.2",
    "express-delay": "^0.2.0",
    "fast-equals": "^5.2.2",
    "luxon": "^3.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.11.4",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-multi-select-component": "^4.3.4",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.30.0",
    "react-search-autocomplete": "^8.5.2",
    "react-spinners": "^0.13.8",
    "redux": "^4.2.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  }
}
